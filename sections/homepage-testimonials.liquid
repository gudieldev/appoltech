{% comment %} {% schema %}
{
	"name": "Testimonials Slider",
	"tag": "section",
	"class": "section-testimonials-slider section-padding",
	"settings": [
		{
			"type": "text",
			"id": "section_title"
			"label": "Section Title"
		},
		{
			"type": "checkbox",
			"id": "show_arrows",
			"label": "Show Arrows?",
		},
		{
			"type": "checkbox",
			"id": "show_indicators",
			"label": "Show Indicators?",
		}
	],
	"blocks": [
		"name": "Testimonail Slide"
		"type": "testimonail",
		"settings": [
			{
				"type": "text",
				"id": "name",
				"label": "Name"
			},
			{
				"type": "richtext",
				"id": "testimonial_content",
				"label": "Testimonial"
			},
			{
				"type": "range",
				"id": "stars",
				"label": "Stars Rate"
			},
			{
				"type": "image_picker",
				"id": "author_image",
				"label": "Testimonial Author Image"
			}
		]
	]
}
{% endschema %} {% endcomment %}